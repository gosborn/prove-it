$(document).ready(function(){
  var finishTime = new Date(); 
  finishTime = new Date("<%= @challenge.challenge_end + @challenge.voting_duration.seconds %>"); 
  $('#votingCountdown').countdown({until: finishTime, onExpiry: function(){
    $.ajax({
      url: "<%= votes_path %>", 
      dataType: "script", 
      type: "GET",
      data: {id: "<%= @challenge.id %>"}});
    },
  });
  
});