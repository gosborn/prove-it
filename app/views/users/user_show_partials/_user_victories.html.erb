<div class="bs-component">
  <% if challenges.size > 0 %>
    <table class="table table-striped table-hover">
      <tbody>
            <h3>Victories!</h3>
            <%= render partial: "challenges/index_partials/list_challenges_rows", locals: {challenges: challenges } %>
      </tbody>
    </table>
    <%= link_to "All Victories", user_victories_path(@user) %>
  <% end %>
</div>